<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.yostar" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="BackgroundMode">
        <param name="browser-package" value="BackgroundMode" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>星柚游记</name>
    <description>
		一款实用且简单的游记工具。
    </description>
    <author email="FutureCoder@aliyun.com" href="https://github.com/SUTFutureCoder/">
        *Chen
		西柚
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <preference name="loadUrlTimeoutValue" value="60000" />
</widget>
